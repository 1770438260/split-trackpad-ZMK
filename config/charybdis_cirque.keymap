    time-to-max-speed-ms = <2000>;
    acceleration-exponent = <0>;
    delay-ms = <0>;
};

/ {
    macros {
    };

    behaviors {
    };

    keymap {
        compatible = "zmk,keymap";

        DEFAULT {
